<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndicatorView">
        <attr name="max_count" format="integer" />
        <attr name="spacing" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="active_color" format="color" />
        <attr name="default_color" format="color" />
        <attr name="active_count" format="integer" />
        <attr name="scale_value" format="integer" />
    </declare-styleable>

    <declare-styleable name="PanoramaImageView">
        <attr name="piv_enablePanoramaMode" format="boolean" />
        <attr name="piv_invertScrollDirection" format="boolean" />
        <attr name="piv_show_scrollbar" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomRatingBar">
        <attr name="maxRating" format="integer" />
        <attr name="emptyStarDrawable" format="reference" />
        <attr name="filledStarDrawable" format="reference" />
        <attr name="emptyLastStarDrawable" format="reference" />
        <attr name="filledLastStarDrawable" format="reference" />
        <attr name="starSpacing" format="dimension" />
    </declare-styleable>
</resources>
